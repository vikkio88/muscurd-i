<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d"
             d:DesignWidth="800"
             d:DesignHeight="450"
             x:Class="Muscurdi.Views.SplashView">
  <DockPanel LastChildFill="True">
    <Border Height="40" BorderBrush="Black" BorderThickness="1" DockPanel.Dock="Top">
      <TextBlock HorizontalAlignment="Center" FontSize="30">Muscurd-I : Password Manager</TextBlock>
    </Border>
    <StackPanel Margin="10" VerticalAlignment="Center">
      <Button Command="{Binding GoToList}">List</Button>
    </StackPanel>
  </DockPanel>
</UserControl>